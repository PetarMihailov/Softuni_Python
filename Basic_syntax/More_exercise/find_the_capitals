input_str = input()
indexes_list = []
for i in range(len(input_str)):
    if input_str[i].isupper():
        indexes_list.append(i)
print(indexes_list)

# Find the Capitals
string = input()
ind = []
for i in range(len(string)):
    if string[i].isupper():
        ind.append(i)
    else:
        continue
print(ind)
